{
  "applicationId": "streaming.rowboat.com",
  "version": "0.0.1",
  "defaultMode": "window",
  "documentRoot": "/www/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "rowboat - helping you with the streams",
      "width": 900,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/www/assets/icon/icon.png"
    }
  },
  "cli": {
    "binaryName": "rowboat",
    "resourcesPath": "/www/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/www/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0"
  }
}